  pod 'Fabric'
  pod 'Crashlytics'
  pod 'MobileVLCKit'
  pod 'PySwiftyRegex', '~> 1.0.0'
  pod 'Alamofire', '~> 4.4'
  # Pods for Hickery

  target 'HickeryTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'HickeryUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
